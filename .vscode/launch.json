{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File with Directory",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            }
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "integratedTerminal",
            "args": [
                "runserver",
                "--noreload",
                "--nothreading"
            ],
            "django": true
        },
        // C++
        {
            "name": "(cppdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/${workspaceFolderBasename}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "miDebuggerPath": "/Users/mkbabb/.vscode/extensions/ms-vscode.cpptools-0.24.0/debugAdapters/lldb/bin/lldb-mi",
            "MIMode": "lldb",
            "terminal": "integrated"
        },
        {
            "name": "(lldb) Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/${workspaceFolderBasename}",
            "args": [
                "-i",
                "data/csv-for-optimization.csv",
                "-o",
                "./data/out-0.csv",
                "--load_file",
                "",
                "--bucket_count",
                "4",
                "--population_count",
                "200",
                "--max_bucket",
                "280",
                "--parent_count",
                "50",
                "--crossover_count",
                "20",
                "--mating_pool_count",
                "10",
                "--mutation_rate",
                "1",
                "--nuke_threshold",
                "10",
                "--nuke_threshold_max",
                "100000",
                "--nuke_mutation_percent",
                "5",
                "--nuke_mutation_percent_max",
                "90",
                "--nuke_growth_rate",
                "2",
                "--nuke_burnout",
                "1",
                "--iterations",
                "10000000",
                "--rng_state",
                "1",
                "--current_best",
                "11213695"
            ],
            "cwd": "${workspaceFolder}",
            "terminal": "integrated"
        }
    ]
}